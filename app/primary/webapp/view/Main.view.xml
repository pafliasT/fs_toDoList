<mvc:View controllerName="com.todolist.primary.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Panel id="todolist-panel" content="{/TodoList}" backgroundDesign="Transparent">
                <content>
                    <GenericTile header="{name}" class="sapUiSmallMargin" />
                </content>
            </Panel>
        </content>
    </Page>
</mvc:View>